\subsection*{Exercise 7.2}

{\label{584413}}

\Huge 1) $\int \frac{2x}{1+x^{2}} \,dx $ 
\newline
\newline
\Huge $u= 1+x^ {2}, du= 2x \,dx$ 
\newline
\newline
\Huge $= \int \frac{1}{u} \,du$
\newline
\newline
\Huge $= \ln |u| + C$

\newline
\Huge 2) $\int \frac{(\ln(x))^{2}}{x} \,dx$ 
\newline
\newline
\Huge $u= \ln {x}, du= \frac{1}{x} \,dx$ 
\newline
\newline
\Huge $= \int u^{2} \,du$
\newline
\newline
\Huge $= \frac{u^{3}}{3} +C $

\newline
\Huge 3) $\int \frac{1}{x+x\ln x} \,dx$ 
\newline
\newline
\Huge $= \int \frac{1}{x(1+\ln x)} \,dx$
\newline
\newline
\Huge $u= 1 +\ln x, du=\frac{1}{x} \,dx$
\newline
\newline
\Huge $= \int \frac{1}{u}\,du$
\newline
\newline
\Huge $= \ln|u| \,du$
\newline
\newline
\Huge $= \ln|1+x^{2}|+ C$

\newline
\Huge 4) $\int \sin x \sin(\cos x) \, dx$ 
\newline
\newline
\Huge $u= \cos x, du=-\sin x \,dx$
\newline
\newline
\Huge $= -\int \sin(u) \,du $
\newline
\newline
\Huge $= -cos(u) $
\Huge $= -cos(cosx) + C$

\newline
\Huge 5) $\int \sin (ax+b) \cos(ax+b) \, dx$ 
\newline
\newline
\Huge $u= \sin (ax+b), du=a \cos (ax+b)  \,dx$
\newline
\newline
\Huge $= \frac{1}{a}\int u \,du $
\newline
\newline
\Huge $= \frac{u^{2}}{2a}$
\newline
\newline
\LARGE $= \frac{\sin(ax+b)^{2}}{2a} + C$

\newline
\Huge 6) $\int \sqrt{ax+b} \,dx$ 
\newline
\newline
\Huge $u= ax+b, du=a \,dx$
\newline
\newline
\Huge $=\frac{1}{a}\int \sqrt{u} \,du$ 
\newline
\newline
\Huge $=\frac{1}{a} \frac {u^\frac{3}{2}} {\frac{3}{2}}$ 
\newline
\newline
\Huge $= \frac {2(ax+b)^\frac{3}{2}}{3a} + C$  

\newline
\Huge 7) $\int x \sqrt{x+2} \,dx$ 
\newline
\newline
\Huge $u= x+2, x=u-2, \,dx=dx$
\newline
\newline
\Huge $=\int (u-2) \sqrt{u} \,du$ 
\newline
\newline
\Huge $=\int (u-2) (u)^{\frac{1}{2}} \,du$ 
\newline
\newline
\Huge $=\int (u)^{\frac{3}{2}}- 2(u)^{\frac{1}{2}} \,du$ 
\newline
\newline
\Huge $=\int (u)^{\frac{3}{2}} du - 2\int (u)^{\frac{1}{2}} \,du$ 
\newline
\newline
\LARGE $=\frac{(u)^{\frac{5}{2}}}{\frac{5}{2}}- 2(\frac{(u)^{\frac{3}{2}}}{\frac{3}{2}})$ 
\newline
\newline 
\Huge $= \frac {2(u)^\frac{5}{2}}{5} -2(\frac {2(u)^\frac{3}{2}}{3})$ 
\newline
\newline
\Huge $= \frac {2(x+2)^\frac{3}{2}}{5} - \frac {4(x+2)^\frac{3}{2}}{3}+ C$  

\newline
\Huge 8) $\int x \sqrt{1+2x^{2}} \,dx$ 
\newline
\newline
\Huge $u= 1+2x^{2}, du= 4x \, dx$
\newline
\newline
\Huge $=\frac{1}{4}\int \sqrt{u} \,du$ 
\newline
\newline
\Huge $= \frac {(u)^\frac{3}{2}}{6}$  
\newline
\newline
\Huge $= \frac {(1+2x^{2})^\frac{3}{2}}{6} + C$  

\newline
\Huge 9) $\int (4x+2) \sqrt{x^{2}+x+1} \,dx$ 
\newline
\newline
\Huge $= \int 2(2x+1) \sqrt{x^{2}+x+1} \,dx$ 
\newline
\newline
\Huge $u= x^{2}+x+1, du= 2x+1 \, dx$
\newline
\newline
\Huge $= 2 \int \sqrt{u} \,du$ 
\newline
\newline
\Huge $= 2 \left(\frac{u^\frac{3}{2}}{\frac{3}{2}}\right) \,du$
\newline
\newline
\Huge $ = \frac{2 u^\frac{3}{2}}{3} + C$
\newline
\newline
\Huge $ = \frac{2(x^{2}+x+1)^\frac{3}{2}}{3} + C$

\newline
\Huge 10) $\int \frac{1}{x-\sqrt{x}} \,dx$ 
\newline
\newline
\Huge $= \int \frac{1}{\sqrt{x}(\sqrt{x}-1)}\,dx$ 
\newline
\newline
\Huge $u= \sqrt{x}-1, dx= 2\sqrt{x}\, dx$
\newline
\newline
\Huge $u= \frac{1}{2} \int \frac{1}{u} \, dx$
\newline
\newline
\Huge $u= \frac{1}{2} \ln |u|$
\newline
\newline
\Huge $u= \frac{1}{2} \ln |\sqrt{x}-1|+ C$

\newline
\Huge 11) $\int \frac{x}{\sqrt{x+4}} \,dx$ 
\newline
\newline
\Huge $u= x+4, x=u-4 du= \, dx$
\newline
\newline
\Huge $ = \int \frac{u-4}{\sqrt{u}} \, du$
\newline
\newline
\Huge $ = \int u^{\frac{1}{2}} - 4u^{\frac{-1}{2}} \, du$
\newline
\newline
\Huge $ = \frac{2u^{\frac{3}{2}}}{3} -8 \sqrt{u} $
\newline
\newline
\Huge $ = \frac{2(x+4)^{\frac{3}{2}}}{3} -8 \sqrt{x+4} + C $

\newline
\Huge 12) $ \int (x^{3}-1)^{\frac{1}{3}} x^{5}  \,dx$ 
\newline
\newline
\Huge $ \int (x^{3}-1)^{\frac{1}{3}} x^{3} x^{2} \,dx$
\newline
\newline
\Huge $u= x^{3}-1, x^{3}=u+1, du= 3x^{2}  \, dx$
\newline
\newline
\Huge $ = \frac{1}{3}\int u^{\frac{1}{3}} (u+1) \,du$
\newline
\newline
\Huge $ = \frac{1}{3}\int u^{1} u^{\frac{1}{3}} + u^{\frac{1}{3}} \,du$
\newline
\newline
\Huge $ = \frac{1}{3}\int u^{\frac{4}{3}}+ u^{\frac{1}{3}} \,du$
\newline
\newline
\Huge $ = \frac{(u)^{\frac{7}{3}}}{7}+\frac{(u)^{\frac{4}{3}}}{4}$
\newline
\newline
\Huge $ = \frac{(x^{3}-1)^{\frac{7}{3}}}{7}+\frac{(x^{3}-1)^{\frac{4}{3}}}{4} + C$

\newline
\Huge $ 13) \int \dfrac {x^{2}}{\left( 2+3x^{3}\right) ^{3}}dx$
\newline
\newline
\Huge $u= 2+3x^{3},du=9x^{2} \, dx$
\newline
\newline
$ =\dfrac {1}{9}\int \dfrac {1}{\left( u\right) ^{3}} \,du$
\newline
\newline
$= \dfrac {1}{9}\left[\dfrac {u^{^{-2}}}{-2}\right]$
\newline
\newline
$= \dfrac {1}{9}\left[\dfrac {(2+3x^{3})^{^{-2}}}{-2}\right]+ C$

\newline
\Huge $ 14) \int \dfrac {1}{x\left( \ln x\right) ^{m}} \,dx$
\newline
\newline
\Huge $u= \ln x ,du=\dfrac{1}{x} \,dx$
\newline
\newline
\Huge $= \int \dfrac{1}{(u)^{m}} \, du $
\newline
\newline
\Huge$= \left[\dfrac {(u)^{^{-m+1}}}{-m+1}\right]$
\newline
\newline
\Huge$= \left[\dfrac {(\ln x)^{^{-m+1}}}{-m+1}\right] + C$

\newline
\Huge $ 15) \int \dfrac {x}{\left( 9-4x^{2}\right) } \,dx$
\newline
\newline
\Huge $u= 9-4x^{2}, du= -8x\,dx$
\newline
\newline
\Huge $= \dfrac{1}{8}\int \dfrac{1}{u} \,du$
\newline
\newline
\Huge $= \dfrac{1}{-8} \ln |u| \,du$
\newline
\newline
\Huge $= \dfrac{1}{-8} \ln |9-4x^{2}| + C$

